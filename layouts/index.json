{{- $.Scratch.Add "index" slice -}}
{{- range site.RegularPages -}}
{{- if and (ne .Layout "search") (ne .Layout "page") -}}
  {{- $.Scratch.Add "index" (dict "title" .Title "permalink" .Permalink "summary" .Summary "content" .Plain) -}}
{{- end -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}